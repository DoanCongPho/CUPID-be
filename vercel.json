{
    "version": 2,
    "functions": {
        "config/wsgi.py": {
           "runtime": "now-python@1.0.0"
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "config/wsgi.py"
        }
    ]
}